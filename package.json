{
  "name": "react-ambiance",
  "version": "0.1.2",
  "files": [
    "dist"
  ],
  "main": "./dist/react-ambiance.umd.js",
  "module": "./dist/react-ambiance.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-ambiance.es.js",
      "require": "./dist/react-ambiance.umd.js"
    }
  },
  "types": "./dist/types/src/index.d.ts",
  "sideEffects": false,
  "description": "React Ambiance",
  "keywords": [
    "ambiance",
    "react",
    "react-ambiance",
    "react-component"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc && tsc-alias",
    "preview": "vite preview",
    "test:watch": "vitest",
    "test": "vitest run",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "storybook": "start-storybook -p 6006",
    "deploy-storybook": "storybook-to-ghpages --source-branch=main"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.7",
    "@storybook/react": "^6.5.15",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.27.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-storybook": "^0.6.8",
    "jest-canvas-mock": "^2.4.0",
    "jsdom": "^21.0.0",
    "prettier": "^2.8.1",
    "storybook-dark-mode": "^2.0.5",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.26.3"
  }
}
